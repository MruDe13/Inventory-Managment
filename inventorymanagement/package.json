{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "main": "./client/desktopApp/main.js",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "body-parser": "^1.19.0",
    "concurrently": "^6.2.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "sqlite3": "^5.0.2",
    "wait-on": "^6.0.0",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "electron": "^13.1.6",
    "electron-builder": "^22.10.7",
    "electron-is-dev": "^2.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "preelectron-pack": "npm run build",
    "pack": "electron-builder --dir",
    "predist": "npm run build",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "preelectron": "npm run build",
    "electron": "electron .",
    "start-server": "nodemon --inspect ./server/index.js",
    "startOver": "nodemon index.js",
    "test-server": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "appId": "monster.inventorymanagement",
    "extends": null,
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "client/desktopApp/main.js",
      "server/**/*",
      "database/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": [
        "nsis"
      ]
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
